<style>
    * {
        margin: 0;
        padding: 0;
        box-sizing: border-box;
        font-family: Arial, sans-serif;
    }

    body {
        background-color: #232323;
        color: #e2e2e2;
        line-height: 1.6;
        height: 100vh;
        text-align: center;
    }

    header {
        background: #27ae60;
        color: #fff;
        padding: 20px;
        text-align: center;
        height: 15vh;
    }

    header h1 {
        font-size: 2.5rem;
    }

    main {
        padding: 20px;
        height: 85vh;
    }

    .marketplace {
        margin-bottom: 40px;
    }

    .marketplace h2 {
        font-size: 1.8rem;
        margin-bottom: 20px;
    }

    .product-list {
        display: grid;
        grid-template-columns: repeat(auto-fill, minmax(250px, 1fr));
        gap: 20px;
    }

    .product-card {
        color: #232323;
        background: #fff;
        padding: 20px;
        border: 1px solid #ddd;
        border-radius: 5px;
        box-shadow: 0 2px 5px rgba(255, 255, 255, 0.1);
    }

    .product-card h3 {
        font-size: 1.5rem;
        margin-bottom: 10px;
    }

    .product-card p {
        margin: 5px 0;
    }

    .product-card button {
        background: #27ae60;
        color: #232323;
        border: none;
        padding: 10px 15px;
        cursor: pointer;
        border-radius: 5px;
        transition: background 0.3s;
    }

    .product-card button:hover {
        background: #2ecc71;
    }

    .add-product {
        margin-top: 40px;
    }

    .add-product h2 {
        margin-bottom: 20px;
    }

    form {
        display: flex;
        flex-direction: column;
    }

    label {
        margin-bottom: 5px;
    }

    input {
        margin-bottom: 15px;
        padding: 10px;
        border: 1px solid #ddd;
        border-radius: 5px;
    }

    button[type="submit"] {
        background: #27ae60;
        color: #fff;
        border: none;
        padding: 10px;
        cursor: pointer;
        border-radius: 5px;
        transition: background 0.3s;
    }

    button[type="submit"]:hover {
        background: #2ecc71;
    }

    footer {
        background: #27ae60;
        color: #232323;
        text-align: center;
        padding: 10px;
        bottom: 0;
        width: 100%;
        height: 7vh;
    }
</style>